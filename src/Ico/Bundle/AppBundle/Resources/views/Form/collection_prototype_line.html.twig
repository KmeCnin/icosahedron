<tr>
{% for field, entry in line %}
    <td class="form-group 
    {% if entry.vars.attr["data-switch"] is defined %}
        {% if entry.vars.attr["data-switch"] == 'custom' %}
        hidden
        {% endif %}
    {% endif %}
    ">
    {% if field in unique_fields %}
        {{ form_widget(entry) }}
    {% else %}
        {{ form_widget(entry, {'attr': {'data-must-be-unique': true} }) }}
    {% endif %}
    </td>
{% endfor %}
    <td>
        <button type="button" class="btn btn-default prototype-remove" data-toggle="tooltip" title="Supprimer">
            <span class="fa fa-minus"></span>
        </button>
    </td>
</tr>